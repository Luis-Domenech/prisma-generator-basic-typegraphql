{
  "name": "prisma-generator-basic-typegraphql",
  "description": "A very simple typegraphql object generator based on prisma schemas",
  "version": "1.1.5",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-generator-basic-typegraphql": "dist/bin.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "npx tsc -w",
    "build": "npx tsc",
    "prepack": "yarn build",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "4",
    "@prisma/generator-helper": "4",
    "@prisma/sdk": "4",
    "chalk": "4.1.2",
    "prettier": "2.5.1",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "27.0.3",
    "@types/node": "17.0.21",
    "@types/prettier": "2.4.2",
    "@types/semver": "^7.3.10",
    "jest": "27.4.7",
    "prisma": "4",
    "semantic-release": "^18.0.1",
    "ts-jest": "27.1.4",
    "typescript": "4.6.2"
  },
  "homepage": "https://github.com/Luis-Domenech/prisma-generator-basic-typegraphql",
  "repository": {
    "type": "git",
    "url": "https://github.com/Luis-Domenech/prisma-generator-basic-typegraphql"
  },
  "author": "Luis F. Domenech Ortiz <luisfabiandomenech@gmail.com>",
  "keywords": [
    "prisma",
    "prisma2",
    "generator"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
